%ul.control_panel.page
  - 3.times do |page|
    %li 
      %strong.large
        = link_to "page #{page}"
      = '-->'
      = link_to "edit"
      |
      = link_to "delete"
      |
      = link_to "add post"
    %ul.control_panel.sub_page
      - 4.times do |sub_page|
        %li
          =link_to "sub page #{sub_page}"
          = '-->'
          = link_to "edit"
          |
          = link_to "delete"
          |
          = link_to "add post"
